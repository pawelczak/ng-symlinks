{
  "name": "ng2-symlinks",
  "version": "0.0.1",
  "description": "Angular2 symlinks test",
  "keywords": [
    "angular2",
    "webpack",
    "karma",
    "jasmine"
  ],
  "author": "Łukasz Pawełczak",
  "license": "MIT",
  "scripts": {
    "postinstall": "typings -- install && npm run symlinks",
    "clean": "rimraf dist",
    "prewebpack:build": "npm run clean",
    "webpack:build": "webpack --inline --colors --progress --display-error-details --display-cached",
    "build": "npm run build:dev",
    "build:dev": "npm run webpack:build -- --config config/webpack.dev.js",
    "build:prod": "npm run webpack:build -- --config config/webpack.prod.js",
    "watch": "npm run build -- --watch",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --inline --progress --port 3000  --content-base src",
    "server:prod": "webpack-dev-server --config config/webpack.prod.js --inline --colors --progress --display-error-details --display-cached --port 3000  --content-base src",
    "start": "npm run server:dev",
    "start:prod": "npm run server:prod",
    "symlinks": "sh symlinks-init.sh"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/platform-server": "2.0.0",
    "@angular/router": "3.2.1",
    "core-js": "^2.2.0",
    "es6-shim": "^0.35.1",
    "ie-shim": "^0.1.0",
    "jquery": "^1.7.4",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "~0.6.23"
  },
  "devDependencies": {
    "@types/es6-shim": "0.0.31",
    "@types/jasmine": "^2.2.33",
    "@types/node": "^6.0.38",
    "@types/protractor": "^1.5.18",
    "angular2-template-loader": "^0.4.0",
    "autoprefixer": "^6.5.1",
    "awesome-typescript-loader": "^1.1.1",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "jquery": "^1.7.4",
    "node-sass": "^3.10.1",
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.0.2",
    "to-string-loader": "^1.1.4",
    "ts-helpers": "^1.1.1",
    "typescript": "2.1.1",
    "typings": "~1.0.3",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-server": "2.1.0-beta.11",
    "webpack-merge": "~1.0.1"
  }
}
